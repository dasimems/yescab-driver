{
  "expo": {
    "name": "YesCab",
    "slug": "YesCab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#E5ECFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yesCab.driver",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.yesCab.driver",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E5ECFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(Paxi) to access your camera.",
          "microphonePermission": "Allow $(Paxi) to access your microphone."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "19118403-7871-48f5-9f06-276fc3120a8a"
      }
    }
  }
}
